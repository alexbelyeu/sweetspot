{
  "extends": "airbnb",
  "rules": {
      "arrow-body-style": 0,
      "arrow-parens": ["error", "as-needed"],
      "func-names": ["error", "never"],
      "no-console": 0,
      "import/prefer-default-export": 0,
      "no-param-reassign": ["error", { "props": false }],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/prefer-stateless-function": 0
  },
  "env": {
      "browser": true
    }
}
